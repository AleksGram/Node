<body>
    <h1 class="nunjucks">Powered by NUNJUCKS</h1>
    <ul id='list'>
    {% for message in messages %}
      <li>
        {{message.addedAt}}<br/>
        <span>{{message.sender}}</span><br/>
        <b>{{message.text}}</b>
      </li>
      {% endfor %}
    </ul>
</body>
</html>